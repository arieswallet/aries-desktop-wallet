{
  "name": "arieswallet",
  "version": "0.0.1",
  "description": "Libra compatible Aries Wallet",
  "author": "nshcore <nshcore@protonmail.com>",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "forge:start": "electron-forge start",
    "forge:package": "electron-forge package",
    "forge:make": "electron-forge make",
    "test:jest": "jest --env=node --config ./jest.config.json --detectOpenHandles",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "arieswallet"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "arieswallet"
      }
    }
  },
  "dependencies": {
    "antd": "^3.25.0",
    "electron": "^7.1.1",
    "kulap-libra": "^2.0.4",
    "lokijs": "^1.5.8",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-jest": "23.2.0",
    "babel-preset-react": "^6.24.1",
    "electron-compile": "^6.4.4",
    "electron-devtools-installer": "^2.1.0",
    "electron-forge": "^5.1.0",
    "electron-prebuilt-compile": "4.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "jest": "23.6.0",
    "react-hot-loader": "^4.12.16",
    "react-test-renderer": "^16.11.0"
  }
}
